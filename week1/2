 def linear_search(a, x, k):
     for i in range(len(a)):
              if a[i] == x:
                 return 1
         return -1
   x = int(input("enter list of elements"))
   a = []
   for i in range(x):
         y = int(input("ele"))
        a.append(y)
  k = int(input("enter element to be searched"))
  pos = linear_search(a, i, k)
  if(pos == -1):
        print("element not found")
  else:
        print("element found")
